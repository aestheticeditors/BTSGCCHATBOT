{
  "name": "insta-chatbot2.0",
  "description": "simple instagram chatbot with insta.js",
  "repository": "https://github.com/aestheticeditors/BTSGCCHATBOT",
  "logo": "https://avatars.githubusercontent.com/u/87944936?v=4",
  "keywords": ["chatbot", "insta chatbot", "instagram chatbot", "nodejs", "bot"],
  "env": {
  "API_ID": {
    "description": "Brain ID from Brainshop.ai",
     "value": "",
     "required" :true
  },
  "API_KEY": {
    "description": "ApiKey from Brainshop.ai",
     "value": "",
     "required" :true
  },
    "USERNAME": {
    "description": "Username/Email ID of Instagram Account",
      "value": "Example: VenomExE@gmail.com",
     "required" :true
  },
  "PASSWORD": {
    "description": "Password Of Instagram Account",
     "value": "Example: VenomExE123",
     "required" :true
  }
},
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    }
  ]
}
